                        @await Html.PartialAsync("_TemplateBoxProduto")
                        <script>ga('ec:setAction', 'detail');</script>
                        @* /Produtos *@

                        <!-- Banner de Categoria -->
                        <!-- Paginação -->
                        <div class="dg-categoria-paginacao dg-categoria-paginacao-footer">
