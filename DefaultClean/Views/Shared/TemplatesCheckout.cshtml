<script id="templateCarrinhoItens" type="text/x-jquery-tmpl">
    <li class="dg-checkout-main-carrinho-titulos dg-desktop-only" role="presentation">
        <span class="dg-checkout-main-carrinho-titulos-produto">Descrição do produto</span>
        <span class="dg-checkout-main-carrinho-titulos-valor">Valor unitário</span>
        <span class="dg-checkout-main-carrinho-titulos-qtd">Qtde</span>
        <span class="dg-checkout-main-carrinho-titulos-total">Valor Total</span>
    </li>

    {for RowCartItem in Carrinho.ItensCart}
    <li class="dg-checkout-main-carrinho-item ItemCart_${RowCartItem.ID_SubProduto}" data-validadecurta="${RowCartItem.IsValidadeCurta}" data-validade="${RowCartItem.DataValidadeFormatada}" data-produto="${RowCartItem.NomeProduto}">
        <div class="dg-checkout-main-carrinho-item-img">
            <img class="dg-mobile-hide" src="/imagens/74x74/${RowCartItem.Foto}" alt="Nome do Produto">
            <img class="dg-desktop-hide" src="/imagens/74x74/${RowCartItem.Foto}" alt="Nome do Produto">
        </div>
        <div class="dg-checkout-main-carrinho-item-produto">
            <strong>${RowCartItem.NomeProduto} ${RowCartItem.ComplementoBrindes}</strong>
                <div>
                    <span class="dg-checkout-main-carrinho-item-crm dg-mobile-hide">
                    <input type="text" placeholder="CRM" class="inputEditCrm_${RowCartItem.ID_SubProduto} dg-mobile-hide" value="${RowCartItem.CRM}" maxlength="8" />
                    <span class="dg-checkout-main-carrinho-item-marca">
                        <span>Marca:</span> <span>${RowCartItem.Marca}</span>
                    </span>
                    {if RowCartItem.IsValidadeCurta == true}
                        <span class="dg-checkout-main-carrinho-item-val">Val. <span>${RowCartItem.DataValidadeFormatada}</span></span>
                    {/if}
                </div>
            </span>
        </div>
        {var ValorView = RowCartItem.ValorUnitario}
        {if RowCartItem.ValorComDescontoUnitario > 0 && RowCartItem.ValorUnitario > RowCartItem.ValorComDescontoUnitario}
            {var ValorView = RowCartItem.ValorComDescontoUnitario}
        {/if}
        <span class="dg-checkout-main-carrinho-item-valor"><span class="dg-mobile-only">Valor unitário.</span>
            {if RowCartItem.ValorUnitario !== RowCartItem.ValorComDescontoUnitario}
                <s>De: R$${RowCartItem.ValorUnitario.FormatCurrency()}</s>
            {/if}
            R$${ValorView.FormatCurrency()}
        </span>
        <span class="dg-checkout-main-carrinho-item-qtd">
            <div class="dg-checkoug-main-carrinho-item-qtd">
                <input type="number" value="${RowCartItem.Qtd}" class="dg-checkoug-main-carrinho-item-qtd-input" id="Qtd_${RowCartItem.ID_SubProduto}" name="Qtd_${RowCartItem.ID_SubProduto}" ID_SubProduto="${RowCartItem.ID_SubProduto}" onKeyPress="if(this.value.length==4) return false;" onkeydown="InputItemCart(this, event);">
                <a href="javascript:void(0)" onclick="SetCarrinhoDelay(this, 'Menos')" class="dg-checkoug-main-carrinho-item-qtd-menos" ID_SubProduto="${RowCartItem.ID_SubProduto}">-</a>
                <a href="javascript:void(0)" onclick="SetCarrinhoDelay(this, 'Mais')" class="dg-checkoug-main-carrinho-item-qtd-mais" ID_SubProduto="${RowCartItem.ID_SubProduto}">+</a>
            </div>
        </span>
        <span class="dg-checkout-main-carrinho-item-total"><span class="dg-mobile-only">Valor Total</span>R$${RowCartItem.ValorTotal.FormatCurrency()}</span>
        <a href="javascript:void(0)" 
         nome="${RowCartItem.NomeProduto}" 
        preco="${RowCartItem.ValorUnitario}" 
        categoria="${RowCartItem.Categoria}"
        marca="${RowCartItem.Marca}"
        onclick="SetCarrinho(this, 'Remove')" class="dg-checkout-main-carrinho-item-apagar dg-tooltip-hover" aria-label="apagar produto" ID_SubProduto="${RowCartItem.ID_SubProduto}">

            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M18 6H6V20C6 20.5523 6.44772 21 7 21H17C17.5523 21 18 20.5523 18 20V6Z" stroke="#CFCFCF" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
            <path d="M4 6H20" stroke="#CFCFCF" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
            <path d="M15 3H9C8.44772 3 8 3.44772 8 4V6H16V4C16 3.44772 15.5523 3 15 3Z" stroke="#CFCFCF" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
            <span class="dg-tooltip">Apagar produto</span>
        </a>
    </li>
    {forelse}
    <li class="dg-checkout-main-carrinho-item">
        <p class="dg-minicarrinho-vazio">Você ainda não adicionou nenhum produto.</p>
    </li>
    {/for}
</script>

