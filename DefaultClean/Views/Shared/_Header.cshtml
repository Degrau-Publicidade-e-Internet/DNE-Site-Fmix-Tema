            <span class="dg-boxbuscaproduto-preco-por">R$ ${String(Produto.Valor.toFixed(2)).replace(".", ",")}</span>
            <div class="dg-boxbuscaproduto-parcelas">
                ${Produto.QTDParcelas}x <span class="dg-small">de</span> R$ ${String(Produto.TotalParcela.toFixed(2)).replace(".", ",")}
                @* <a href="javascript:void(0)" class="dg-broxproduto-parcelas-icone dg-tooltip-hover" aria-label="Valor em ${Produto.QTDParcelas}x sem juros para compras com parcela mínima de R$50,00. Consulte os juros aplicados em sua compra na escolha da forma de pagamento.">
                    <span class="dg-icon" role="presentation">i</span>
                    <span class="dg-tooltip">
                        Valor em 3x sem juros para compras com <b>parcela mínima de R$ 50,00</b>. <span>
                            *Consulte os juros
                            aplicados em sua compra na escolha da forma de pagamento.
                        </span>
                    </span>
                </a> *@
            </div>
        </div>


        {if Produto.Controlado == true || Produto.VendaRestrita == true }
        <a  href="${Produto.URLProduto}" class="dg-boxbuscaproduto-btn-controlado" rel="${Produto.ID_SubProduto}.ToString()">Saiba Mais</a>
        {elseif Produto.Estoque < 1}
         <div class="dg-boxproduto-compra">
            <a href="javascript:void(0)" class="dg-boxproduto-compra-btn jsModalAviseMe" aria-label="Avisar chegada de ${Produto.Produto}" rel="${Produto.ID_SubProduto}" tabindex="0">
                <span class="dg-icon dg-icon-avise-me"></span>
                Avise-me
            </a>
        </div>
        {else}
        <div class="dg-boxbuscaproduto-compra">
            <div class="dg-boxbuscaproduto-qtd">
                <input type="number" class="dg-boxbuscaproduto-qtd-input" name="" id="QTD_${Produto.ID_SubProduto}"
                    value="1" multiplo="1" maxlength="2" tabindex="0" onKeyPress="if(this.value.length==4) return false;">
                <a href="javascript:void(0)" class="dg-boxbuscaproduto-qtd-menos" rel="${Produto.ID_SubProduto}" tabindex="0">-</a>
                <a href="javascript:void(0)" class="dg-boxbuscaproduto-qtd-mais" rel="${Produto.Estoque}" tabindex="0">+</a>
            </div>
            <a href="javascript:void(0)" class="dg-boxbuscaproduto-compra-btn BtComprarProduto" aria-label="Comprar ${Produto.Produto}" id_subproduto="${Produto.ID_SubProduto}" tabindex="0">
                Comprar <span class="dg-icon dg-icon-cart"></span>
            </a>
        </div>

        {/if}
    </div>



</script>

<script id="template-AutoCompleteBuscaProdutos" type="text/x-jquery-tmpl">

    {for produto in Produtos}

        <li>
            <a href="${produto.URLProduto}" class="dg-header-busca-auto-lista-item">
                <figure class="dg-img">
                    <img src="@Html.Domain()/imagens/200x200/${produto.Foto}" alt="${produto.Produto}">
                </figure>
                <div class="dg-conteudo">
                    <div class="dg-subtitulo">
                        ${produto.Marca}
                    </div>

                    <div class="dg-titulo">
                       ${produto.Produto} ${produto.CplTraduzido} ${produto.ComplementoBrindes}
                    </div>
                    <p class="dg-preco">
                        R$ ${String(produto.ValorComDesconto.toFixed(2)).replace(".", ",")}
