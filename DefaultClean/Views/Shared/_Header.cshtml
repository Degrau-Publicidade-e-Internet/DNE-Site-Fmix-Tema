@using DNE.Site.ViewServices;
@inject DNE.Site.ViewServices.HeaderView oHeaderView

@{
    var BannerMensagemClasse = "";
    var OHeader = oHeaderView.GetHeader(ViewData["BodyID"] != null ? ViewData["BodyID"].ToString() : "");
}

<web-antiforgery-token />

@if (OHeader.BannerMensagemTopo.IsAtivo)
{
    BannerMensagemClasse = "dg-tem-banner-mensagem-top";
}

<header class="dg-header @BannerMensagemClasse">

    <input type="hidden" name="FBAPP_ID" id="FBAPP_ID" value="636648873464236" />
    <input type="hidden" name="FBAPP_ID" id="G_client_id" value="1028179406320-c2tcqbvotsek6o48gnq8h44kuvc2ui6h.apps.googleusercontent.com" />

    <!-- Acessibilidade -->
    <a href="#main" class="dg-ac-irpara">
        Ir para o Conteúdo
    </a>
    <!-- /Acessibilidade -->
